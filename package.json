{
  "name": "new-football-server",
  "version": "1.0.0",
  "description": "server for sorayama's football app",
  "private": true,
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "scripts": {
    "start": "egg-scripts start --title=egg-server-new-football-server",
    "startd": "egg-scripts start --daemon --title=egg-server-new-football-server",
    "stop": "egg-scripts stop --title=egg-server-new-football-server",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test-local": "egg-bin test",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "autod": "autod",
    "lint": "tslint --project . -c tslint.json",
    "clean": "ets clean"
  },
  "dependencies": {
    "egg": "^2.25.0",
    "egg-cors": "^2.2.2",
    "egg-mongoose": "^3.2.0",
    "egg-scripts": "^2.11.0",
    "egg-validate": "^2.0.2",
    "http-status-codes": "^1.4.0",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.146",
    "@types/mocha": "^5.2.7",
    "@types/mongoose": "^5.5.30",
    "@types/node": "^12.12.0",
    "@types/supertest": "^2.0.0",
    "autod": "^3.1.0",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.14.0",
    "egg-ci": "^1.13.0",
    "egg-mock": "^3.24.2",
    "prettier": "^1.19.1",
    "tslib": "^1.9.0",
    "tslint": "^5.20.0",
    "tslint-config-egg": "^1.0.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.7.2"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "eslintIgnore": [
    "coverage"
  ],
  "author": {
    "name": "SoraYama",
    "email": "sorayamahou@gmail.com"
  },
  "license": "MIT"
}
